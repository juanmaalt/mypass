{
  "name": "MyPass",
  "productName": "MyPass",
  "description": "Password saver app made with Electron and React",
  "version": "1.5.0",
  "author": {
    "name": "Owl"
  },
  "homepage": "./",
  "main": "main.js",
  "build": {
    "appId": "com.owl.mypass",
    "productName": "MyPass",
    "directories": {
      "output": "builds/"
    },
    "mac": {
      "target": "default",
      "category": "public.mypass.category.dev",
      "icon": "/src/assets/icons/icon.icns"
    },
    "win": {
      "target": "portable",
      "icon": "/src/assets/icons/icon.ico"
    }
  },
  "scripts": {
    "prod": "webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "webpack --config webpack.dev.config.js --mode production",
    "pack-lin": "electron-packager . MyPass --platform=linux --arch=x64 --no-prune",
    "pack-win": "electron-packager . MyPass --platform=win32 --arch=x64 --no-prune --out=./builds --overwrite",
    "pack-mac": "electron-packager . MyPass --platform=darwin --arch=x64 --no-prune --out=./builds --overwrite",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist-all": "electron-builder -mw",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "crypto-js": "^4.0.0",
    "firebase": "^7.14.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-tooltip": "^4.2.6",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "reselect": "^4.0.0",
    "styled-components": "^5.1.0",
    "sweetalert": "^2.1.2",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^3.5.3",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.1",
    "electron-devtools-installer": "^3.0.0",
    "electron-packager": "^14.2.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.11.0"
  }
}
